      ]
    },
    {
      "cell_type": "code",
      "metadata": {
        "id": "dr0luKoGibVO",
        "colab_type": "code",
        "colab": {}
      },
      "source": [
        "from sklearn.ensemble import RandomForestClassifier\n",
        "rf = Pipeline(steps=[('preprocessor', preprocessor),\n",
        "                      ('classifier', RandomForestClassifier())])"
      ],
      "execution_count": 0,
      "outputs": []
    },
